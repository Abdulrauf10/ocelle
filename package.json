{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rimraf .next && npm run generate:gql && next build",
    "build:analyzer": "ANALYZE=true next build",
    "start": "next start",
    "start:direct": "next start -p 80",
    "test": "vitest",
    "generate:gql": "graphql-codegen --config .graphqlrc.ts",
    "lint": "next lint",
    "prettier": "prettier --write \"./**/*.{ts,tsx}\"",
    "ts-node": "dotenvx run --env-file=.env --env-file=.env.local -- ts-node -r tsconfig-paths/register",
    "setup": "npm run ts-node -- src/setup.ts",
    "cron": "npm run ts-node -- src/cron.ts",
    "typeorm": "npm run ts-node -- node_modules/typeorm/cli.js",
    "typeorm:mysql": "npm run typeorm -- -d src/AppDataSource/MySQL.ts",
    "migration:create": "npm run typeorm -- migration:create src/migrations/${npm_config_name}",
    "migration:generate": "npm run typeorm:mysql -- migration:generate src/migrations/${npm_config_name}",
    "migration:run": "npm run typeorm:mysql -- migration:run",
    "migration:show": "npm run typeorm:mysql -- migration:show",
    "migration:revert": "npm run typeorm:mysql -- migration:revert",
    "i18n:generate": "node scripts/import18.mjs",
    "i18n:export": "node scripts/export18.mjs"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.6.4",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react": "^0.26.13",
    "@google-cloud/text-to-speech": "^5.3.0",
    "@mui/material": "^5.15.4",
    "@mui/x-date-pickers": "^6.19.0",
    "@next/third-parties": "^14.2.7",
    "@saleor/auth-sdk": "^1.0.1",
    "@stripe/react-stripe-js": "^2.7.1",
    "@stripe/stripe-js": "^3.4.0",
    "@tanstack/react-query": "^5.37.1",
    "clone-deep": "^4.0.1",
    "clsx": "^2.1.1",
    "country-codes-list": "^1.6.11",
    "cron": "^3.1.6",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dayjs": "^1.11.11",
    "debounce": "^2.1.0",
    "decimal.js": "^10.4.3",
    "deep-equal": "^2.2.3",
    "dotenv": "^16.4.5",
    "editorjs-html": "^3.4.3",
    "ejs": "^3.1.10",
    "framer-motion": "^11.1.9",
    "google-gax": "^4.3.6",
    "html-to-text": "^9.0.5",
    "i18n-iso-countries": "^7.11.3",
    "ioredis": "^5.4.1",
    "joi": "^17.12.1",
    "js-cookie": "^3.0.5",
    "mysql2": "^3.9.7",
    "next": "^14.2.3",
    "next-intl": "^3.13.0",
    "nodemailer": "^6.9.13",
    "object-to-formdata": "^4.5.1",
    "pluralize": "^8.0.0",
    "react": "^18",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18",
    "react-fast-marquee": "^1.6.4",
    "react-hook-form": "^7.51.4",
    "react-merge-refs": "^2.1.1",
    "react-router-dom": "^6.23.1",
    "react-text-mask": "^5.5.0",
    "react-toastify": "^10.0.5",
    "reflect-metadata": "^0.2.2",
    "round-to": "^5.0.0",
    "sharp": "^0.33.3",
    "short-uuid": "^5.2.0",
    "store2": "^2.14.3",
    "stripe": "^15.6.0",
    "swiper": "^11.1.1",
    "ts-invariant": "^0.10.3",
    "typeorm": "^0.3.19",
    "typeorm-naming-strategies": "^4.1.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@ganmahmud/tailwindcss-language-variant": "^1.0.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@next/bundle-analyzer": "^14.1.0",
    "@testing-library/react": "^16.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/clone-deep": "^4.0.4",
    "@types/deep-equal": "^1.0.4",
    "@types/ejs": "^3.1.5",
    "@types/html-to-text": "^9.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.15",
    "@types/path-browserify": "^1.0.2",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18",
    "@types/react-text-mask": "^5.4.14",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "exceljs": "^4.4.0",
    "jsdom": "^24.1.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "vitest": "^2.0.3"
  }
}

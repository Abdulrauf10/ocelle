{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate:gql && next build",
    "build:analyzer": "ANALYZE=true next build",
    "start": "next start",
    "start:direct": "next start -p 80",
    "generate:gql": "graphql-codegen --config .graphqlrc.ts",
    "lint": "next lint",
    "prettier": "prettier --write \"./**/*.{ts,tsx}\"",
    "ts-node": "dotenvx run --env-file=.env --env-file=.env.local -- ts-node -r tsconfig-paths/register",
    "setup": "npm run ts-node -- src/setup.ts",
    "cron": "npm run ts-node -- src/cron.ts",
    "typeorm": "npm run ts-node -- node_modules/typeorm/cli.js",
    "typeorm:mysql": "npm run typeorm -- -d src/AppDataSource/MySQL.ts",
    "migration:create": "npm run typeorm -- migration:create src/migrations/${npm_config_name}",
    "migration:generate": "npm run typeorm:mysql -- migration:generate src/migrations/${npm_config_name}",
    "migration:run": "npm run typeorm:mysql -- migration:run",
    "migration:show": "npm run typeorm:mysql -- migration:show",
    "migration:revert": "npm run typeorm:mysql -- migration:revert"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.34.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react": "^0.26.5",
    "@mui/material": "^5.15.4",
    "@mui/x-date-pickers": "^6.19.0",
    "@saleor/auth-sdk": "^1.0.1",
    "@stripe/react-stripe-js": "^2.6.2",
    "@stripe/stripe-js": "^3.0.10",
    "clone-deep": "^4.0.1",
    "clsx": "^2.1.0",
    "cron": "^3.1.6",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "deep-equal": "^2.2.3",
    "dotenv": "^16.4.1",
    "editorjs-html": "^3.4.3",
    "framer-motion": "^10.18.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.1",
    "js-cookie": "^3.0.5",
    "mysql2": "^3.7.0",
    "next": "14.0.4",
    "next-intl": "^3.5.2",
    "object-to-formdata": "^4.5.1",
    "pluralize": "^8.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-fast-marquee": "^1.6.4",
    "react-hook-form": "^7.49.3",
    "react-merge-refs": "^2.1.1",
    "react-router-dom": "^6.22.3",
    "react-text-mask": "^5.5.0",
    "react-toastify": "^10.0.5",
    "reflect-metadata": "^0.2.1",
    "sharp": "^0.33.2",
    "store2": "^2.14.3",
    "stripe": "^15.1.0",
    "swiper": "^11.0.5",
    "ts-invariant": "^0.10.3",
    "typeorm": "^0.3.19",
    "typeorm-naming-strategies": "^4.1.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@next/bundle-analyzer": "^14.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/clone-deep": "^4.0.4",
    "@types/deep-equal": "^1.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/path-browserify": "^1.0.2",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-text-mask": "^5.4.14",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
